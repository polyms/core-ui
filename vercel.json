{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm nx build docs",
  "outputDirectory": "dist/docs",
  "devCommand": "pnpm nx serve docs",
  "installCommand": "pnpm install",
  "framework": null,
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
